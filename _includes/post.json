{"id":"{{ forloop.index }}","url":"{{site.baseurl}}{{ post.url }}","title":"{{ post.title }}","body":"{{post.content | strip_html | replace_newline_with_space | remove:'"' }}","excerpt":"{{ post.content | strip_html | replace_newline_with_space | truncatewords: 56 | remove:'"' }}","excerpt_mobile":"{{ post.content | strip_html | replace_newline_with_space | truncatewords: 18 | remove:'"' }}","categories":"{% for category in post.categories %}{{ category }}{% unless forloop.last %}, {% endunless %}{% endfor %}"}
